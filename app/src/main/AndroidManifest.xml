<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aicteapp">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AICTE">

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.aicteapp.StatsActivity"/>
        <activity android:name="com.example.aicteapp.DashBoardActivity"/>
        <activity android:name="com.example.aicteapp.InstitutesActivity"/>
        <activity android:name="com.example.aicteapp.GraphsAndChartsActivity"/>
        <activity android:name="com.example.aicteapp.AnnouncementsActivity"/>
        <activity android:name="com.example.aicteapp.PolicyActivity"/>
        <activity android:name="com.example.aicteapp.AboutActivity"/>
        <activity android:name="com.example.aicteapp.NextActivity"/>
        <activity android:name="com.example.aicteapp.NewsActivity"/>

    </application>

</manifest>